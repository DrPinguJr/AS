@page
@model dit230680c_AS.Pages.AuditLogsModel
@{
    ViewData["Title"] = "Audit Logs";
}

<h2>Audit Logs</h2>

<table class="table">
    <thead>
        <tr>
            <th>Timestamp</th>
            <th>User</th>
            <th>Activity</th>
            <th>IP Address</th>
            <th>User Agent</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model.AuditLogs)
        {
            <tr>
                <td>@log.Timestamp</td>
                <td>@log.UserId</td>
                <td>@log.Activity</td>
                <td>@log.IpAddress</td>
                <td>@log.UserAgent</td>
            </tr>
        }
    </tbody>
</table>
